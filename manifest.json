{
  "manifest_version": 2,
  "name": "URL Popup",
  "version": "1.0",
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {

  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
